{
  "name": "@pantheon-systems/pcc-vue-sdk",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Cloud Vue SDK",
  "version": "2.0.4",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "tsup",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint . --fix && prettier --write . && tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "esbuild-plugin-globals": "^0.2.0",
    "eslint": "^8.46.0",
    "eslint-config-pcc-custom": "workspace:*",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vue": "npm:vue@^3.3.4",
    "vue-2": "npm:vue@^2.7.14",
    "vue-3": "npm:vue@^3.3.4"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1.0.0-rc.1",
    "vue": "^2.6.11 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@pantheon-systems/pcc-sdk-core": "workspace:*",
    "@vue/apollo-composable": "^4.0.0-beta.7",
    "vue-demi": "latest",
    "vue-markdown-render": "^2.0.1"
  },
  "sideEffects": false
}
