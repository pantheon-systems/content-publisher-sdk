{
  "name": "@pantheon-systems/pcc-react-sample-library",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Cloud Sample Component Library for React",
  "version": "3.1.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/css/*.css",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -f -u 1 node_modules/@pantheon-systems/pds-toolkit-react/**/*.css src/**/*.css dist/css/",
    "dev": "pnpm run build --watch",
    "build": "pnpm run clean && tsup && pnpm run copy-files",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint . --fix && prettier --write . && tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@types/node": "^20.10.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-pcc-custom": "workspace:*",
    "eslint-plugin-storybook": "^0.6.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.7",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@pantheon-systems/pcc-react-sdk": "*",
    "@pantheon-systems/pcc-sdk-core": "*",
    "@pantheon-systems/pds-toolkit-react": "1.0.0-dev.55"
  },
  "sideEffects": false
}
