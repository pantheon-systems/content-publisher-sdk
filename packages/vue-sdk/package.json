{
  "name": "@pantheon-systems/pcc-vue-sdk",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Cloud Vue SDK",
  "version": "3.0.5",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    },
    "./components/style.css": "./dist/components/index.css",
    "./nuxt": {
      "types": "./dist/platforms/nuxt.d.ts",
      "require": "./dist/platforms/nuxt.cjs",
      "import": "./dist/platforms/nuxt.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "nuxt": [
        "./dist/platforms/nuxt.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "tsup",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint . --fix && prettier --write . && tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.10.7",
    "esbuild-plugin-globals": "^0.2.0",
    "eslint": "^8.56.0",
    "eslint-config-pcc-custom": "workspace:*",
    "h3": "^1.10.0",
    "sass": "^1.69.7",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3",
    "unplugin-vue": "^4.5.2",
    "vite": "^5.0.12",
    "vue": "npm:vue@3.0.0 - 3.3.4",
    "vue-2": "npm:vue@^2.7.16",
    "vue-3": "npm:vue@3.0.0 - 3.3.4"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1.0.0-rc.1",
    "h3": "^1.8.2",
    "vue": "^2.6.11 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "h3": {
      "optional": true
    }
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@pantheon-systems/pcc-sdk-core": "latest",
    "@vue/apollo-composable": "4.0.0-beta.12",
    "floating-vue": "2.0.0-beta.24",
    "graphql": "^16.8.1",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "html-url-attributes": "^3.0.0",
    "query-string": "^8.1.0",
    "rehype-raw": "^6.1.1",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "vue-demi": "latest"
  },
  "sideEffects": false
}
