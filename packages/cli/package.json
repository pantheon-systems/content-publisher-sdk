{
  "name": "@pantheon-systems/cpub-cli",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Publisher CLI",
  "version": "5.2.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "pcc",
    "cpub",
    "nextjs",
    "react",
    "pantheon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pantheon-systems/content-publisher-sdk"
  },
  "bin": {
    "pcc": "./dist/index.js",
    "cpub": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates",
    "scripts"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup --env.NODE_ENV production",
    "build:staging": "tsup --env.NODE_ENV staging",
    "build:test": "tsup --env.NODE_ENV test",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "preinstall": "node scripts/checkNodeVersion.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@pantheon-systems/cpub-sdk-core": "workspace:*",
    "axios": "^1.13.5",
    "bluebird": "^3.7.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.12",
    "dom-parser": "^1.1.5",
    "fs-extra": "^11.2.0",
    "get-port": "^7.1.0",
    "google-auth-library": "^9.13.0",
    "googleapis": "^129.0.0",
    "inquirer": "^8.2.6",
    "nunjucks": "^3.2.4",
    "octokit": "^4.1.2",
    "open": "^9.1.0",
    "ora": "^6.3.1",
    "package-json": "^8.1.1",
    "query-string": "^8.2.0",
    "server-destroy": "^1.0.1",
    "showdown": "^2.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.3",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/bluebird": "^3.5.42",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "29.5.1",
    "@types/node": "^20.11.21",
    "@types/nunjucks": "^3.2.6",
    "@types/server-destroy": "^1.0.3",
    "@types/showdown": "^2.0.6",
    "@types/tmp": "^0.2.6",
    "@types/yargs": "^17.0.33",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.2",
    "eslint-config-pcc-custom": "workspace:*",
    "jest": "29.5.0",
    "tmp": "^0.2.5",
    "ts-jest": "29.1.0",
    "tsup": "^8.2.4"
  }
}
