{
  "name": "@pantheon-systems/pcc-react-sample-library",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Cloud Sample Component Library for React",
  "version": "2.0.18",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/css/*.css",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -f -u 1 node_modules/@pantheon-systems/pds-toolkit-react/**/*.css src/**/*.css dist/css/",
    "dev": "pnpm run build --watch",
    "build": "pnpm run clean && tsup && pnpm run copy-files",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint . --fix && prettier --write . && tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.46.0",
    "eslint-config-pcc-custom": "workspace:*",
    "rimraf": "^5.0.5",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@pantheon-systems/pcc-react-sdk": "latest",
    "@pantheon-systems/pcc-sdk-core": "latest",
    "@pantheon-systems/pds-toolkit-react": "1.0.0-dev.55"
  },
  "sideEffects": false
}
